S.S.4,  \ Infusion
  S1,
    #Z2: IF A(4) = 0 [@NOINFUSION, @INFUSION]
         @NOINFUSION: ---> S5
         @INFUSION: ---> S2
  S2,
    0.01": SET D(G) = H; SET E(G) = 5; ADD G, Y; SET D(G) = -987.987; SET E(G) = -987.987 ---> S3
  S3,
    A(4)*100#T: SET D(G) = H; SET E(G) = 6; ADD G; SET D(G) = -987.987; SET E(G) = -987.987 ---> S4
  S4,
   ((A(5)*100)-A(4)*100)#T: Z1 ---> S1
  S5,
    0.03": ---> S6
  S6,
    (A(5)*100)#T: Z1 ---> S1






