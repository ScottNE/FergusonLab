    1#R^LeftLeverIn: SET D(G) = H; SET E(G) = 35; ADD G, A(13); SET D(G) = -987.987; SET E(G) = -987.987 ---> S2
    #Z1: ---> S1
    #Z3: ---> S3
  S3,     \ No-drug period
